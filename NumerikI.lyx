\series medium
(Kondition)
\begin_inset CommandInset label
LatexCommand label
name "sec:SuD-Kondition"

\end_inset


\end_layout

\begin_layout Standard
Sei 
\begin_inset Formula $E\subseteq\mathbb{R}^{k_{1}}\times\ldots\times\mathbb{R}^{k_{l}}$
\end_inset

 offen und 
\begin_inset Formula $f:E\to\mathbb{R}^{n}\setminus\left\{ 0\right\} $
\end_inset

 differenzierbar.
\begin_inset Newline newline
\end_inset

Dann gilt für alle 
\begin_inset Formula $z\in\mathbb{R}^{k_{1}}\times\ldots\times\mathbb{\mathbb{R}}^{k_{l}}$
\end_inset

 mit 
\begin_inset Formula $z_{j}\not=0$
\end_inset

 für 
\begin_inset Formula $j\in\left\{ 1,\ldots,l\right\} $
\end_inset

.
\begin_inset Formula 
\begin{align*}
\varrho_{f\left(\overline{z}\right)}\left(f\left(z\right)\right) & \le\sum_{j=1}^{l}\kappa_{j}\left(f,z\right)\varrho_{\overline{z}}\left(z_{j}\right)+R\left(z-\overline{z}\right) &  & \text{für }\overline{z}\to z
\end{align*}

\end_inset

Dabei ist
\begin_inset Formula 
\begin{align*}
\varrho_{\overline{z}_{j}}\left(z_{j}\right) & :=\frac{\norm{z_{j}-\overline{z}_{j}}_{j}}{\norm{z_{j}}_{j}} &  & \text{der relative Fehler der Daten,}\\
\varrho_{f\left(\overline{z}\right)_{j}}\left(f\left(z\right)\right) & :=\frac{\norm{f\left(z\right)-f\left(\overline{z}\right)}}{\norm{f\left(z\right)}} &  & \text{der relative Fehler der Lösung,}\\
\kappa_{j}\left(f,z\right) & :=\frac{\opnorm{\DD_{j}f\left(z\right)}_{j}\cdot\norm{z_{j}}_{j}}{\norm{f\left(z\right)}} &  & \text{der Verstärkungsfaktor}
\end{align*}

\end_inset

und 
\begin_inset Formula $\norm ._{j}$
\end_inset

 und 
\begin_inset Formula $\norm .$
\end_inset

 sind Normen auf 
\begin_inset Formula $\mathbb{R}^{k_{j}}$
\end_inset

 beziehungsweise 
\begin_inset Formula $\mathbb{R}^{n}$
\end_inset

 für 
\begin_inset Formula $j\in\left\{ 1,\ldots,l\right\} $
\end_inset

.
 Für 
\begin_inset Formula $R$
\end_inset

 gilt:
\begin_inset Formula 
\begin{align*}
R\left(z-\overline{z}\right) & =o_{0}\left(\sum_{j=1}^{l}\norm{z_{j}-\overline{z}_{j}}_{j}\right)
\end{align*}

\end_inset

Der Ausdruck 
\begin_inset Formula $\kappa_{j}\left(f,z\right)$
\end_inset

 heißt 
\emph on
Kondition von 
\begin_inset Formula $f$
\end_inset

 im Punkte 
\begin_inset Formula $z$
\end_inset

 in Richtung 
\begin_inset Formula $j$
\end_inset


\emph default
.
\end_layout

\begin_layout Subsubsection*
Beweis
\end_layout

\begin_layout Standard
Die Differenzierbarkeit von 
\begin_inset Formula $f$
\end_inset

 liefert:
\begin_inset Formula 
\begin{align*}
f\left(\overline{z}\right) & =f\left(z\right)+\sum_{j=1}^{l}\DD_{j}f\left(z\right)\left(\overline{z}_{j}-z_{j}\right)+\overline{R}
\end{align*}

\end_inset

Damit folgt:
\begin_inset Formula 
\begin{align*}
\overline{R} & =o_{0}\left(\sum_{j=1}^{l}\norm{\overline{z}_{j}-z_{j}}_{j}\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{f\left(\overline{z}\right)-f\left(z\right)}{\norm{f\left(z\right)}} & =\frac{1}{\norm{f\left(z\right)}}\sum_{j=1}^{l}\DD_{j}f\left(z\right)\left(\overline{z}_{j}-z_{j}\right)+\frac{1}{\norm{f\left(z\right)}}\overline{R}
\end{align*}

\end_inset

Es folgt:
\begin_inset Formula 
\begin{align*}
\varrho_{f\left(\overline{z}\right)}\left(f\left(z\right)\right) & =\frac{\norm{f\left(\overline{z}\right)-f\left(z\right)}}{\norm{f\left(z\right)}}\le\frac{1}{\norm{f\left(z\right)}}\sum_{j=1}^{l}\norm{\DD_{j}f\left(z\right)\left(\overline{z}_{j}-z_{j}\right)}+\underbrace{\frac{1}{\norm{f\left(z\right)}}\norm{\overline{R}}}_{=:R}\le\\
 & \le\frac{1}{\norm{f\left(z\right)}}\sum_{j=1}^{l}\frac{\norm{z_{j}}_{j}}{\norm{z_{j}}_{j}}\norm{\abs{\DD_{j}f\left(z\right)}}_{j}\norm{\overline{z}_{j}-z_{j}}_{j}+R=\\
 & =\sum_{j=1}^{l}\kappa_{j}\left(f,z\right)\varrho_{\overline{z}}\left(z_{j}\right)+R
\end{align*}

\end_inset

Dabei erfüllt 
\begin_inset Formula $R$
\end_inset

 die geforderten Eigenschaften.
