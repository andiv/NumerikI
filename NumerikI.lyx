\begin_inset Formula 
\begin{align*}
r_{n}\left(x\right) & =\frac{1}{n!}\int_{a}^{x}f^{\left(n+1\right)}\left(t\right)\left(x-t\right)^{n}\dd t=\\
 & =\frac{1}{n!}\int_{a}^{b}f^{\left(n+1\right)}\left(t\right)\left(x-t\right)^{n}\Theta\left(x-t\right)\dd t=\\
 & =\frac{1}{n!}\int_{a}^{b}f^{\left(n+1\right)}\left(t\right)\left(x-t\right)_{+}^{n}\dd t
\end{align*}

\end_inset

Wende nun 
\begin_inset Formula $R$
\end_inset

 auf die Taylor-Entwicklung an.
 Da 
\begin_inset Formula $R\left(p\right)=0$
\end_inset

 für alle 
\begin_inset Formula $p\in\mathbb{P}_{n}$
\end_inset

 gilt, folgt:
\begin_inset Formula 
\begin{align*}
R\left(f\right) & =R\left(r_{n}\right)=\\
 & =\frac{1}{n!}\left(\int_{a}^{b}f^{\left(n+1\right)}\left(t\right)\left(.-t\right)_{+}^{n}\dd t\right)=\\
 & =\frac{1}{n!}\left(\sum_{i=0}^{n}\alpha_{i}\int_{a}^{b}f^{\left(n+1\right)}\left(t\right)\left(x_{i}-t\right)_{+}^{n}\dd t-\int_{a}^{b}\int_{a}^{b}f^{\left(n+1\right)}\left(t\right)\left(x-t\right)_{+}^{n}\dd t\dd x\right)=\\
 & \stackrel{\text{Fubini}}{=}\frac{1}{n!}\left(\int_{a}^{b}f^{\left(n+1\right)}\left(t\right)\left(\sum_{i=0}^{n}\alpha_{i}\left(x_{i}-t\right)_{+}^{n}-\int_{a}^{b}\left(x-t\right)_{+}^{n}\dd x\right)\dd t\right)=\\
 & =\frac{1}{n!}\int_{a}^{b}f^{\left(n+1\right)}\left(t\right)R\left(\left(.-t\right)_{+}^{n}\right)\dd t=\\
 & =\int_{a}^{b}f^{\left(n+1\right)}\left(t\right)K\left(t\right)\dd t
\end{align*}

\end_inset


