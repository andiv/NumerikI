 & \text{Algorithmus }A &  & \text{Algorithmus }B\\
f_{1} & :=p\cdot p & q_{1} & :=p\cdot p\\
f_{2} & :=f_{1}+q & q_{2} & :=q_{1}+q\\
f_{3} & :=\sqrt{f_{m}} & q_{3} & :=\sqrt{q_{2}}\\
